FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      moment.3.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [3 -3 0 0 0 0 0];

internalField   uniform 3.29634e-15;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 3.29634e-15;
    }
    outlet
    {
        type            zeroGradient;
    }
}


