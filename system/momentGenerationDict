FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      momentGenerationDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases
(
    particles
);

boundaries
{
    inlet
    {
        type    fixedValue;
        value   uniform 0;
    }
    defaultFaces
    {
        type    empty;
    }
    walls
    {
        type    zeroGradient;
    }
    outlet
    {
        type    zeroGradient;
    }
}

particles
{
    type                alphaAndDiameter;
    weightDimension     [0 -3 0 0 0 0 0];
    abscissaDimension   [1 0 0 0 0 0 0];

    "internal|default"
    {
        node0
        {
            rho     uniform [1 -3 0 0 0 0 0] 1684;
            dia     uniform [0 1 0 0 0 0 0] 125e-6;
            alpha   0.6;
        }
    }

    setRegions
    {}
}


